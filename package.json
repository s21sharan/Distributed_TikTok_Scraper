{
  "name": "tiktok-scraper-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t tiktok-scraper-ui .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "worker": "node worker/worker.js",
    "worker:dev": "nodemon worker/worker.js",
    "deploy:vercel": "vercel --prod",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "@types/uuid": "^9.0.7",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.5.10",
    "@types/node-cron": "^3.0.11",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
} 